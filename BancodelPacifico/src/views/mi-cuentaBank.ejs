<!DOCTYPE html>
<html lang="en">

<head>
    <%-include("partials/header")%>
        <title>New book </title>
</head>

<body>
    <%-include("partials/navigation") %>
        <div class="container p-4">
            <div class="container">
                <% cuentas.forEach(function (cuenta){ %>
                    <div class="container">
                        <div class="row row-cols-2">
                          <div class="card">Nombre :</div>
                          <div class="card"><%= cuenta.cuenta.nombre %></div>
                          <div class="card">C.I :</div>
                          <div class="card"><%= cuenta.cuenta.ci %></div>
                          <div class="card">Nombre del Banco :</div>
                          <div class="card"><%= cuenta.cuenta.banco %></div>
                          <div class="card">Monto :</div>
                          <div class="card"><%= cuenta.cuenta.monto %></div>
                        </div>
                      </div>
                    <% }) %>
            </div>
        </div>
</body>

</html>